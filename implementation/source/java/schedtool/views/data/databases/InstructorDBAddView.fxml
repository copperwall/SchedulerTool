<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER_RIGHT" prefHeight="190.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.data.databases.InstructorDBAddController">
  <children>
    <HBox alignment="CENTER_RIGHT" fillHeight="false" prefHeight="100.0" prefWidth="200.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
      <children>
        <Label text="Cal Poly User ID" />
      </children>
    </HBox>
    <HBox alignment="CENTER_RIGHT" fillHeight="false" prefHeight="100.0" prefWidth="200.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4">
      <children>
        <Label text="WTU" />
      </children>
      <rotationAxis>
        <Point3D y="1.0" z="1.0" />
      </rotationAxis>
    </HBox>
    <TextField fx:id="userID" GridPane.columnIndex="1" GridPane.rowIndex="3">
      <GridPane.margin>
        <Insets left="5.0" />
      </GridPane.margin>
    </TextField>
    <TextField fx:id="wtu" GridPane.columnIndex="1" GridPane.rowIndex="4">
      <GridPane.margin>
        <Insets left="5.0" />
      </GridPane.margin>
    </TextField>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="5">
      <children>
        <Label alignment="CENTER_RIGHT" text="Active" />
      </children>
    </HBox>
    <Label text="Add Instructor">
      <font>
        <Font size="16.0" />
      </font>
      <GridPane.margin>
        <Insets left="10.0" />
      </GridPane.margin>
    </Label>
    <CheckBox fx:id="active" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="5">
      <GridPane.margin>
        <Insets left="5.0" />
      </GridPane.margin>
    </CheckBox>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="RIGHT" GridPane.rowIndex="6" GridPane.valignment="CENTER">
      <children>
        <Button mnemonicParsing="false" onAction="#submit" text="Submit">
          <HBox.margin>
            <Insets right="5.0" />
          </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      </children>
      <GridPane.margin>
        <Insets right="10.0" />
      </GridPane.margin>
    </HBox>
    <Label text="First Name" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
    <TextField fx:id="firstName" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextField fx:id="lastName" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <Label text="Last Name" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
    <Label fx:id="lblWtuError" text="" textFill="RED" GridPane.columnIndex="2" GridPane.rowIndex="4" />
  </children>
  <columnConstraints>
    <ColumnConstraints minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
    <ColumnConstraints minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
    <ColumnConstraints minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
  </rowConstraints>
</GridPane>
