<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER_RIGHT" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.data.databases.InstructorDBEditController">
  <children>
    <HBox alignment="CENTER_RIGHT" fillHeight="false" prefHeight="100.0" prefWidth="200.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
      <children>
        <Label text="Cal Poly User ID" />
      </children>
    </HBox>
    <HBox alignment="CENTER_RIGHT" fillHeight="false" prefHeight="100.0" prefWidth="200.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
      <children>
        <Label text="WTU" />
      </children>
      <rotationAxis>
        <Point3D y="1.0" z="1.0" />
      </rotationAxis>
    </HBox>
    <TextField fx:id="wtu" GridPane.columnIndex="1" GridPane.rowIndex="2">
      <GridPane.margin>
        <Insets left="5.0" />
      </GridPane.margin>
    </TextField>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="3">
      <children>
        <Label alignment="CENTER_RIGHT" text="Active" />
      </children>
    </HBox>
    <TextField editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <GridPane.margin>
        <Insets left="5.0" />
      </GridPane.margin>
    </TextField>
    <Label text="Edit Instructor">
      <font>
        <Font size="16.0" />
      </font>
      <GridPane.margin>
        <Insets left="10.0" />
      </GridPane.margin>
    </Label>
    <CheckBox fx:id="active" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3">
      <GridPane.margin>
        <Insets left="5.0" />
      </GridPane.margin>
    </CheckBox>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="RIGHT" GridPane.rowIndex="4" GridPane.valignment="CENTER">
      <children>
        <Button mnemonicParsing="false" onAction="#edit" text="Submit">
          <HBox.margin>
            <Insets right="5.0" />
          </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      </children>
      <GridPane.margin>
        <Insets right="10.0" />
      </GridPane.margin>
    </HBox>
  </children>
  <columnConstraints>
    <ColumnConstraints minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
    <ColumnConstraints minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
    <ColumnConstraints minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" />
  </rowConstraints>
</GridPane>
