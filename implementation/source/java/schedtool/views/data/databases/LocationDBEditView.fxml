<GridPane prefHeight="285.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.data.databases.LocationDBEditController">
  <children>
    <Label alignment="CENTER_RIGHT" contentDisplay="LEFT" focusTraversable="false" text="Edit Location" textOverrun="ELLIPSIS" underline="false" wrapText="false" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <font>
        <Font size="16.0" />
      </font>
      <GridPane.margin>
        <Insets left="10.0" />
      </GridPane.margin>
    </Label>
    <HBox id="HBox" alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <children>
        <Label alignment="CENTER" text="Building">
          <HBox.margin>
            <Insets right="10.0" fx:id="x1" />
          </HBox.margin>
        </Label>
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="3">
      <children>
        <Label text="Room" HBox.margin="$x1" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
      <children>
        <Label text="Capacity" HBox.margin="$x1" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="5">
      <children>
        <Label text="Equipment" HBox.margin="$x1" />
      </children>
    </HBox>
    <TextField fx:id="buildingText" alignment="CENTER" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextField fx:id="roomText" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <TextField fx:id="capacityText" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
    <TextField fx:id="equipmentText" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
    <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="6">
      <children>
        <Button mnemonicParsing="false" onAction="#submit" text="Submit" HBox.margin="$x1" />
        <Button mnemonicParsing="false" onAction="#cancel" text="Cancel">
          <HBox.margin>
            <Insets />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="2">
      <children>
        <Label text="Label">
          <HBox.margin>
            <Insets right="10.0" fx:id="x1" />
          </HBox.margin>
        </Label>
      </children>
    </HBox>
    <TextField fx:id="buildingNumberText" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
