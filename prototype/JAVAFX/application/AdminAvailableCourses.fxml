<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?scenebuilder-stylesheet application.css?>

<AnchorPane prefHeight="480.0" prefWidth="750.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="InstructorDBController">
  <children>
    <BorderPane prefHeight="242.0" prefWidth="220.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <bottom>
        <GridPane>
          <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="200.0" GridPane.columnIndex="1">
              <children>
                <Button mnemonicParsing="false" onAction="#addInstructor" text="Load From:" />
                <ChoiceBox prefWidth="106.0">
                  <items>
                    <FXCollections fx:factory="observableArrayList">
                      <String fx:value="Item 1" />
                      <String fx:value="Item 2" />
                      <String fx:value="Item 3" />
                    </FXCollections>
                  </items>
                </ChoiceBox>
                <Button mnemonicParsing="false" onAction="#deleteInstructor" text="Load Suggested">
                  <HBox.margin>
                    <Insets left="5.0" />
                  </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#cancelChanges" text="Cancel">
                  <HBox.margin>
                    <Insets left="5.0" />
                  </HBox.margin>
                </Button>
              </children>
            </HBox>
            <Button mnemonicParsing="false" text="Save" />
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <padding>
            <Insets bottom="20.0" />
          </padding>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <BorderPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
          </BorderPane.margin>
        </GridPane>
      </bottom>
      <center>
        <TableView prefHeight="200.0" prefWidth="200.0">
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn editable="false" maxWidth="5000.0" minWidth="0.0" prefWidth="59.0" text="Prefix" />
            <TableColumn editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="82.0" text="Course No." />
            <TableColumn editable="false" maxWidth="5000.0" minWidth="0.0" prefWidth="263.0" text="Course Title" />
            <TableColumn editable="true" minWidth="0.0" prefWidth="71.0" text="Sections" />
            <TableColumn editable="false" minWidth="0.0" styleClass="grayed-out" text="Suggested Sections" />
          </columns>
          <BorderPane.margin>
            <Insets left="10.0" right="10.0" />
          </BorderPane.margin>
        </TableView>
      </center>
      <top>
        <Label graphicTextGap="4.0" text="Available Courses">
          <font>
            <Font size="16.0" />
          </font>
          <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" top="5.0" />
          </BorderPane.margin>
        </Label>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
